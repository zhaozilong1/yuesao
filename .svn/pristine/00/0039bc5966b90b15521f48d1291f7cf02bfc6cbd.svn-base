.warp{
  width: 100%;
  height: 100%;
  overflow-y: auto;
  overflow-x: hidden;
  header{
    height:0.58rem;
    line-height: 0.58rem;
    border-bottom: 1px solid #e5e5e5;
    background-color: #fff8fa;
    text-align: center;
    font-size:0.36rem;
    color: #333333;
    padding: 0.25rem;
    .back{
      display: block;
      width:0.28rem;
      height: 0.28rem;
      background: url("../images/doctor-01/icon_back.png") no-repeat;
      background-size:0.28rem 0.28rem;
      margin-top:0.17rem;

    }
  }
  .main{
    nav{
      ul{
        display:flex;
        li{
          height: 0.96rem;
          line-height: 0.96rem;
          flex: 1;
          float: left;
          text-align: center;
          a{
            display:block;
            font-size: 0.28rem;
          }

        }
      }
    }
    .change{
      color:red;
      border-bottom: 2px solid red;
    }
    .dingdan_list{
      .combo{
        display: none;
        .combo_bottom{
          border-bottom: 1px solid #e5e5e5;
          background-color: #fff;
          padding: 0 0.2rem;
          box-sizing: border-box;
          .pic{
            a{
              display: block;
              width: 1.8rem;
              height: 1.8rem;
              background: url("../images/img_yishengtixing.png") no-repeat;
              background-size:1.8rem 1.8rem;
              margin-top: 0.15rem;
              margin-right: 0.15rem;
              padding-bottom: 0.15rem;
            }
          }
          .right{

            float: left;
            width: 70%;
            .top{
              padding-top: 0.1rem;
              .user{
                border-right: 1px solid #e5e5e5;
                font-size: 0.32rem;
                padding-right: 0.2rem;
                color: #333333;
              }
              .zhuzhi{
                font-size: 0.24rem;
                padding-left: 0.2rem;
                padding-top: 0.04rem;
                color: #666666;
              }
              .detail{
                font-size: 0.24rem;
                color: #ff4400;

              }
            }
            P{
              font-size: 0.24rem;
              line-height: 2.5;
              margin-top: 0.1rem;
              &:last-child{
                //color: #ff4400;
              }
              img{
                float: right;
                width: 0.2rem;
                padding-top: 0.3rem;
              }
            }



          }
        }
      }
      .tishi{
        width: 100%;
        line-height: 2;
        text-align: center;
        font-size: 0.25rem;
        margin-top: 1rem;
        img{
          display: block;
          margin: auto;
        }
      }
      .selected{
        display:block;
      }
    }
  }
}