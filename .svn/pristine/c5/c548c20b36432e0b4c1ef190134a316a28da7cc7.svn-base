*{
    padding: 0;
    margin: 0;
}
ul li{
    list-style: none;
}
.warp{
    width:100%;
    height:250px;
    display:flex;
    justify-content: space-between;
    alignment: center;

}
.warp_left{
    width:250px;
    display:flex;
    background:pink;
}
.warp_left img{
    width:100%;
    height:100%;
}
.warp_right{
    position: relative;
    flex:1;
}

.warp_right h2{
    padding:10px;
    display:block;
    color:#333333;
    font-size: 28px;
    margin-left: 18px;
}
.warp_right h2:nth-child(1){
    margin-top: 23px;
}
.warp_right p{
    margin-top: 50px;
    font-size: 28px;
    color:#FF4400;
    margin-left: 18px;
}
.footer{
    float:right;
    margin-right: 10px;
        margin-top: -128px;
}
.footer button{
    width:130px;
    height:55px;
    text-align: center;
    line-height: 55px;
    border:0;
    font-size: 24px;
    outline: none;
    background:#FF4400;
    color:white;
    border-radius: 12px;
}
.center{
    width:100%;
    height:450px;
    margin-top: 20px;
}
.center_top{
    width:667px;
    height:450px;
    line-height: 50px;
    margin:0 auto;
    font-size: 28px;
    color:#666666;
}
.center_top p{
    padding: 10px;
}
.bottom_num{
    display: inline-block;
    width: 141px;
    white-space: nowrap;
    position: absolute;
    left: 185px;
    bottom: 7px;
}
.bottom_num button{
    outline: none;
    border:0px;
    background-color:transparent;
    font-size: 35px;
}
.bottom_num button img{
    width: 32px;
    height: 32px;
}
.bottom_num span{
    font-size: 30px;
}