body{
  background-image: linear-gradient(to bottom, #fff, #ffc6eb);
}
.warp{
  width: 100%;
  height: 100%;
  overflow: hidden;
  header{
    height:1.08rem;
    line-height: 1.08rem;
    text-align: center;
    border-bottom: 1px solid #e5e5e5;
    font-size: 0.32rem;
    background-color: #ffffff;
    padding: 0 0.25rem;
    box-sizing: border-box;
    .back{
      display: block;
      width:0.28rem;
      height: 0.28rem;
      background: url("../images/doctor-01/icon_back.png") no-repeat;
      background-size:0.28rem 0.28rem;
      margin-top:0.1rem;
      margin-top: 0.4rem;
      float: left;
    }
  }
  .birth{
    width: 100%;
    height: 1.28rem;
    margin-top: 0.1rem;
    border-bottom: 1px solid #e5e5e5;
    border-top: 1px solid #e5e5e5;
    line-height: 1.28rem;
    font-size: 0.3rem;
    padding-left: 0.25rem;
    padding-right: 0.25rem;
    background-color: #ffffff;
    box-sizing: border-box;
    input{
      width: 1.8rem;
      height: 0.46rem;
      border: 1px #dedede solid;
      border-radius: 0.1rem;
      margin-left: 1.5rem;
      text-align: center;
    }
  }
.sex{
  padding: 1.55rem 1.44rem;
  p{
    cursor:pointer;
    margin-top:0.2rem;
  }
  .nan{
   .pic_nan{
     display: block;
     width:1.6rem;
     height: 1.6rem;
     background: url("../images/man.png") no-repeat;
     background-size:1.6rem 1.6rem;
   }
    p{
      text-align: center;
    }

  }
  .nv{
    .pic_nv{
      display: block;
      width:1.6rem;
      height: 1.6rem;
      background: url("../images/woman.png") no-repeat;
      background-size:1.6rem 1.6rem;
    }
    p{
      text-align: center;
    }

  }
}
  .next{
    text-align: center;
    margin-top: 1.3rem;
    button{
      width: 90%;
      height: 0.9rem;
      background-color: #fff;
      border: 1px solid #e5e5e5;
      border-radius: 5px;
      color: #e77fc3;
    }
  }
  .box_bottom{
    margin-top: 80px;
    height: 100%;
    display: none;
    header{
      height:1.08rem;
      line-height: 1.08rem;
      text-align: center;
      border-bottom: 1px solid #e5e5e5;
      font-size: 0.32rem;

    }
    .call{
      height:0.9rem;
      line-height:0.9rem;
      padding: 0 0.2rem;
      border-bottom: 1px solid #e5e5e5;
      border-top: 1px solid #e5e5e5;
      font-size: 0.32rem;
      margin-top:0.2rem;
    }
    .nickname{
      text-align: center;
      margin-top: 2.65rem;

      input{
        width: 90%;
        height: 0.9rem;
        border: 1px solid #e5e5e5;
        text-indent: 2em;
        border-radius: 4px;
      }
    }
    .next{
      text-align: center;
      margin-top: 2.65rem;

      button{
        width: 90%;
        height: 0.9rem;
        border: 1px solid #e5e5e5;
        border-radius: 4px;
        background-color: #fff;
        color: #e77fc3;
      }
    }
  }
}