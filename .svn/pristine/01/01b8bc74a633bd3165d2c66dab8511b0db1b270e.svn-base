.warp{
  width: 100%;
  height: 100%;
  overflow-y: auto;
  overflow-x: hidden;
  header{
    height: 0.58rem;
    line-height: 0.58rem;
    border-bottom: 1px solid #e5e5e5;
    background-color: #fff8fa;
    text-align: center;
    font-size:0.36rem;
    color: #333333;
    padding: 0.25rem;
    .back{
      display: block;
      width:0.28rem;
      height: 0.28rem;
      background: url("../images/doctor-01/icon_back.png") no-repeat;
      background-size:0.28rem 0.28rem;
      margin-top:0.17rem;

    }
  }
  .main{
    margin-bottom: 0.96rem;
    .combo{

      .combo_bottom{
        border-bottom: 1px solid #e5e5e5;
        background-color: #fff;
        padding: 0 0.25rem;
        box-sizing: border-box;
        .pic{
          a{
            display: block;
            width: 0.8rem;
            height: 0.8rem;
            background: url("../images/touxiang02.png") no-repeat;
            background-size:0.8rem 0.8rem;
            margin-top: 0.15rem;
            margin-right: 0.15rem;
          }
        }
        .right{
          float: left;
          width: 85%;
          .top{
            padding-top: 0.1rem;
            h3{
              font-size: 0.24rem;
              font-weight: 400;
            }
            span{
              color: #999999;
              font-size: 0.24rem;
            }
          }
          .text{
            font-size: 0.28rem;
            color: #0f3773;
            padding: 0.2rem 0;
          }


        }
      }
      .questions_box{
        background-color: #fff;
        padding: 0 0.25rem;
        .questions_in{
          height:2.34rem;
          padding-left: 0.48rem;
          border-bottom: 1px solid #e5e5e5;

          .questions_top{
            height: 0.64rem;
            line-height: 0.64rem;
            border-bottom: 1px solid #e5e5e5;
            .left{
              .user{
                border-right: 1px solid #e5e5e5;
                padding: 0 0.2rem;
                padding-left: 0.3rem;
                position: relative;
                &:before{
                  display: block;
                  content:"";
                  width:0.2rem;
                  height: 0.2rem;
                  background: url("../images/doctor-01/icon_05.png") no-repeat;
                  background-size:0.2rem 0.2rem;
                  position: absolute;
                  top: 4px;
                }

              }
              .yuesao{
                color: #ff7676;
                padding-left: 0.1rem;
              }
            }
            .time{
              color: #999999;
              font-size:0.24rem;
            }
          }
          .text{
            font-size:0.24rem;
            color: #666666;
            padding-top: 0.1rem;
          }
        }
      }

    }
    .gauge{
      margin-top: 0.1rem;
    }


  }

  .tishi{
    width: 100%;
    line-height: 2;
    text-align: center;
    font-size: 0.25rem;
    margin-top: 2rem;
    display: none;
  }
  .tishi img{
    width: 2rem;
    display: block;
    margin: auto;
  }


}