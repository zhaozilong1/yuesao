/*zhushi*/
.dingdan_box{
    float: right;
    width: 100%;
    padding: 0 100px;
    box-sizing: border-box;
    margin-bottom: 50px;
}
.dingdan_box a{
    text-decoration: none;
    color: #fff;
    padding: 10px;
    background-color: #cc6c6c;
    font-size: 25px;
    border-radius: 10px;
}
.dingdan_box .ip{
    background-color: red;
    line-height: 188px;
    padding: 0.1rem 0.2rem;
    color: #fff;
    border-radius: 4px;
    font-size: 0.26rem;
    margin-top: 67px;
}
.dingdan_box .coo{
    background-color: #65d465;
    padding: 0.1rem 0.2rem;
    color: #fff;
    border-radius: 4px;
    font-size: 0.26rem;
    float: right;
    margin-top: 67px;
}
.dingdan_box a:last-child{
    background-color: #65d465;
}
