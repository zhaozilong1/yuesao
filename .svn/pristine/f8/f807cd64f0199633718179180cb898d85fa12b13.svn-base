*{
    padding: 0;
    margin: 0;
}
ul li{
    list-style: none;
}
body{
    width:100%;
    height:1130px;
    background:#fafafa;
}
.warp{
    width: 100%;
    height: 82px;
    display: flex;
    background:#f0f0f0;
}
.warp_cen{
    width:667px;
    height:82px;
    margin:0 auto;
    display:flex;
    justify-content: space-between;
    align-items: center;
}
.warp_cen h2{
    font-size: 36px;
    color: #333333;
}
.warp span{
    font-size: 32px;
    color:#333333;
}
.center{
    width:100%;
    height:299px;
    margin-top: 5px;
    display:flex;
}
.center_cen{
    width:667px;
    height:299px;
    margin:0 auto;
    display:flex;
    justify-content: space-between;
    background:white;
    border-radius: 10px;
    border-bottom:white; box-shadow:0 15px 10px #f0efef;
}
.center_left{
    width:182px;
    height:299px;
    display:flex;
    position: relative;
}
.center_left img{   
    margin-left: 47px;
    width:45px;
    height:86px;
    
}
.center_left span{
    position:absolute;
    left:55px;
    top:10px;
    font-size: 24px;
    color:white;
}
.center_right{
    flex:1px;
}


.name_right{
    height:40px;
    margin-top: 28px;
}
.name_right input{
    disabled:disabled;
    background-color: #fff;
}
.name_right h2{
    color:#333333;
    float: left;
    font-size: 30px;
}
.name_right input{
    width:280px;
    height:36px;
    border: 0;   
    float: left;
    font-size: 30px;
    color:#333333;   
    outline: none;  
    margin-left: 10px;
}
.name_right span{
    width:1px;
    height:30px;
    background:#c0c0c0;
}
.name_1 input{
    background:#f0f0f0;
}

