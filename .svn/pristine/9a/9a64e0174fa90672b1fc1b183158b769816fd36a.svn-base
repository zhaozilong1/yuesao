*{
    padding: 0;
    margin: 0;
}
ul li{
    list-style: none;
}
.warp{
    width: 100%;
    height: 96px;
    display: flex;
    background:#fff8fa;
}
.warp_cen{
    width:667px;
    height:96px;
    margin:0 auto;
    display:flex;
    justify-content: space-between;
    align-items: center;
}
.warp_cen h2{
    font-size: 36px;
    color: #333333;
}
.one{
    width:100%;
    height:12px;
    background:#f0f0f0;
    border-top:solid 2px #e5e5e5;
    border-bottom:solid 2px #e5e5e5;
}
.center{
    width:100%;
    height:142px;
    padding-top:20px;
}
.center_cen{
    width:667px;
    height:120px;
    margin:0 auto;
}
.center_left{
    width:120px;
    height:120px;
    float: left;
}
.center_left img{
    width:100%;
    height:100%;
}
.center_right{
    width:520px;
    height:120px;
    float: right;
    position: relative;
}
.center_top{
    width:100%;
    height:36px;
    line-height: 36px;
    margin-top:10px;
}
.center_top h2{
    color:#333333;
    font-size: 32px;
    float:left;
}
.center_top span{
    display:block;
    width:1px;
    height:36px;
    display:block;
    float:left;
    margin-left: 29px;
    background:#e5e5e5;
}
.center_top i{
    font-style: normal;
    display:inline-block;
    float:left;
    margin-left: 29px;
    font-size: 24px;
    color:#666666;
}
.center_bottom{
    float:left;
    color:#666666;
    font-size: 24px;
    margin-top: 25px;
}
.center_img img{
    position:absolute;
    right:0;
    bottom:45px;
}
.two{
    width:100%;
    height:6px;
    background:#f0f0f0;
    border-top:solid 2px #e5e5e5;
    border-bottom:solid 2px #e5e5e5;
}
.footer{
    width:100%;
    height:96px;
    background:#fff8fa;
    margin-top: 12px;
    padding-top: 12px;
}
.footer_cen{
    width:667px;
    height:80px;
    line-height: 80px;
    text-align: center;
    margin:0 auto;
    font-size: 32px;
    color:#ffffff;
    background:#ff8383;
}