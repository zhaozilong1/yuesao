body{
  background-image: linear-gradient(to bottom, #fff, #ffc6eb);
}
.warp{
  width: 100%;
  height: 100%;
  //overflow: hidden;
  header{
    height:1.08rem;
    line-height: 1.08rem;
    text-align: center;
    border-bottom: 1px solid #e5e5e5;
    font-size: 0.32rem;
    background-color: #ffffff;
    padding: 0 0.25rem;
    box-sizing: border-box;
    .back{
      display: block;
      width:0.28rem;
      height: 0.28rem;
      background: url("../images/doctor-01/icon_back.png") no-repeat;
      background-size:0.28rem 0.28rem;
      margin-top:0.1rem;
      margin-top: 0.4rem;
      float: left;
    }
  }
  .expected{
    width: 100%;
    height: 1.28rem;
    border-bottom: 1px solid #e5e5e5;
    line-height: 1.28rem;
    font-size: 0.3rem;
    padding-left: 0.62rem;
    padding-right: 0.62rem;
    background-color: #ffffff;
    box-sizing: border-box;
    input{
      width: 2.38rem;
      height: 0.46rem;
      border: 1px #dedede solid;
      border-radius: 0.1rem;
      margin-left: 0.3rem;
    }
  }
  .text{
    width: 100%;
    height: 4.14rem;
    padding: 0 0.2rem;
    box-sizing: border-box;
    span{
      font-size: 0.26rem;
      color: #ff4400;
      display: block;
      margin-top: 0.79rem;
      margin-left: 0.26rem;
    }
    p{
      font-size: 0.22rem;
      text-indent: 2em;
      line-height: 2;
      margin-top: 0.2rem;

    }
  }
  .next{
    text-align: center;
    margin-top: 2.65rem;
    button{
      width: 90%;
      height: 0.9rem;
      background-color: #fff;
      border: 1px solid #e5e5e5;
      border-radius: 5px;
      color: #e77fc3;
    }
  }

  .box_bottom{
    margin-top: 80px;
    height: 100%;
    display: none;
    header{
      height:1.08rem;
      line-height: 1.08rem;
      text-align: center;
      border-bottom: 1px solid #e5e5e5;
      font-size: 0.32rem;

    }
    .call{
      height:0.9rem;
      line-height:0.9rem;
      padding: 0 0.2rem;
      border-bottom: 1px solid #e5e5e5;
      border-top: 1px solid #e5e5e5;
      font-size: 0.32rem;
      margin-top:0.2rem;
    }
    .nickname{
      text-align: center;
      margin-top: 2.65rem;

      input{
        width: 90%;
        height: 0.9rem;
        border: 1px solid #e5e5e5;
        text-indent: 2em;
        border-radius: 4px;
      }
    }
    .next{
      text-align: center;
      margin-top: 2.65rem;

      button{
        width: 90%;
        height: 0.9rem;
        border: 1px solid #e5e5e5;
        border-radius: 4px;
        background-color: #fff;
        color: #e77fc3;
      }
    }
  }

}